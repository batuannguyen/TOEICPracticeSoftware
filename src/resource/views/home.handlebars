<h1> MAIN LAYOUT </h1>
<div>
    <a href="http://localhost:4000/login">Link</a>
</div>
<div>
    <ul>
        {{#each player_data}}
        <li><span style="color: red">{{this.name}}:</span>{{this.position}}</li>
        {{/each}}
    </ul>
    <ul>
        {{#each user_data}}
        <li>{{this.username}}:{{this.password}}</li>
        {{/each}}
    </ul>
    <p>Status: {{status}}</p>
</div>
